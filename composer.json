{
  "name": "svalentinf/paypal-sdk",
  "description": "PayPal's PHP SDK for REST APIs",
  "keywords": [
    "paypal",
    "payouts",
    "rest",
    "sdk",
    "orders",
    "payments",
    "subscriptions"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "http://github.com/svalentif/paypal-sdk/",
  "require": {
    "paypal/paypalhttp": "~1.0.1"
  },
  "authors": [
    {
      "name": "PayPal",
      "homepage": "https://github.com/paypal/PAYOUTS-PHP-SDK/contributors"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "PaypalPayoutsSDK\\": "lib/",
      "Sample\\": "samples/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "unit": "phpunit --testsuite unit",
    "integration": "phpunit --testsuite integration"
  }
}
